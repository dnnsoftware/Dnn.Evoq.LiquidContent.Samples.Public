{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es5",
        "jsx": "react",
        "baseUrl": ".",
        "paths": {
          "@/*": ["./src/*"],
          "*": ["./node_modules/@types/*"]
        }
    },
    "include": ["**/*"],
    "exclude": [
        "node_modules"
    ]
}